{
  "name": "envesto",
  "version": "1.0.0",
  "description": "rest services, api, admin-view, restful services, nosql",
  "main": "index.js",
  "scripts": {
    "test": "ng test",
    "build:doc": "apidoc -i src/ -o doc/",
    "start:doc": "npm run build:doc && node api-doc-server",
    "start:dev": "nodemon",
    "start:build": "rimraf ./dist && tsc",
    "start:staging": "cross-env NODE_ENV=staging node -r ./dist/index.js",
    "start:development": "cross-env NODE_ENV=development node -r ./dist/index.js",
    "doc": "node api-doc-server"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "public"
    ],
    "exec": "ts-node src/index.ts"
  },
  "keywords": [
    "restfull",
    "nosql",
    "apis"
  ],
  "author": "sanjay",
  "license": "ISC",
  "_moduleAliases": {
    "@services": "dist/services",
    "@config": "dist/config",
    "@interfaces": "dist/interfaces",
    "@middlewares": "dist/middlewares",
    "@models": "dist/models",
    "@utils": "dist/utils",
    "@routes": "dist/routes",
    "@controllers": "dist/controllers"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-session": "^1.17.4",
    "@types/helmet": "^4.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.177",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "@types/node": "^16.11.26",
    "@types/pino": "^6.3.11",
    "@types/pino-pretty": "^4.7.1",
    "@types/redis": "^2.8.32",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.13",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@types/async-redis": "^1.1.2",
    "@types/mongoose": "^5.11.97",
    "@types/mysql": "^2.15.19",
    "async-redis": "^2.0.0",
    "aws-sdk": "^2.1037.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "custom-env": "^2.0.1",
    "ejs": "^3.1.6",
    "ejs-locals": "^1.0.2",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "express-session": "^1.17.2",
    "fast-two-sms": "^3.0.0",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^6.0.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
    "mysql2": "^2.3.0",
    "otp-generator": "^2.0.1",
    "pino": "^6.13.3",
    "pino-pretty": "^7.0.1",
    "redis": "^3.1.2",
    "rimraf": "^3.0.2",
